@model Videohosting.Models.Channel
@{
    ViewBag.Title = "Channel";
}
<div id="sub-button">
    @using (Ajax.BeginForm("Subscribe", new AjaxOptions { UpdateTargetId = "sub-button" }))
    {   
        <input type="hidden" name="channelId" value="@Model.Id"/>
        <input type="hidden" name="userName" value="@Model.User.UserName"/>
        <input type="submit" value="Subscribe" />
    }
</div>

